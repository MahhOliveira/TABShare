public class TablaturaTest {
 
 @Test
 public void getNotasOk() {
 String conteudo =
 “e|---------------------------------------------------|” +
 “B|------------------------7---5-7-8-7-8-7-5-7-5-3----|” +
 “G|--7-6-4-6-7---7-6-4-6-7---7---------------------7--|” +
 “D|------------7--------------------------------------|” +
 “A|---------------------------------------------------|” +
 “E|---------------------------------------------------|”;
 
 String titulo = “Rompendo em fé”;
 String descricao = “Melodia introdutória”;
 
 Tablatura tablatura = new Tablatura(conteudo, titulo, descricao);
 assertEquals(“D-C#-B-C#-D-A-D-C#-B-C#-D-F#-D-E-F#-G-F#-G-F#-E-
 F#E-D-D- “, tablatura.getNotas());
 }
 
 // outros métodos de testes exercitando diferentes cenários
